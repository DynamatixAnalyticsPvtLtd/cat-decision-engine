{
  "name": "@dynamatix/cat-decision-engine",
  "version": "1.0.0",
  "description": "A powerful Node.js library for building and executing configurable workflows with validation and task execution capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "ftest": "jest --testPathPattern=loan-approval-workflow",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "seed:workflows": "ts-node src/scripts/init-workflows.ts",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "workflow",
    "engine",
    "node",
    "typescript",
    "validation",
    "task-execution",
    "mongodb",
    "decorators"
  ],
  "author": "Dynamatix",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynamatix/cat-decision-engine.git"
  },
  "bugs": {
    "url": "https://github.com/dynamatix/cat-decision-engine/issues"
  },
  "homepage": "https://github.com/dynamatix/cat-decision-engine#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "dotenv": "^16.5.0",
    "mongoose": "^8.1.3",
    "reflect-metadata": "^0.2.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
